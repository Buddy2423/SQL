CREATE TABLE warzone (id INTEGER PRIMARY KEY AUTOINCREMENT,name TEXT, score INTEGER, kills INTEGER, rank INTEGER);

INSERT INTO warzone (name, score, kills, rank) VALUES ("Buddy4k", "2780", 21, 20);
